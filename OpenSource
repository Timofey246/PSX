	local eggs = {}
	for i, v in pairs(game:GetService("ReplicatedStorage")["__DIRECTORY"].Eggs:GetChildren()) do
		if v:IsA("Folder") then
			table.insert(eggs, "ðŸ”¥ "..v.Name)
